            </div>
            <div>
                <label for="direccion">Dirección: </label><br>
                <input type="text" class="form-control" name="direccion" 
                placeholder="direccion"
                value="{{ request.form['direccion'] }}">
                <br>
            </div>
            <div>
                <label for="correo_electronico">Correo eléctronico: </label><br>
                <input type="text" class="form-control" name="correo_electronico" 
                placeholder="correo_electronico"
                value="{{ request.form['correo_electronico'] }}">
                <br>
            </div>
            <div>
                <label for="contrasena">Descripción: </label><br>
                <input type="text" class="form-control" name="contrasena" 
                placeholder="contrasena"
                value="{{ request.form['contrasena'] }}">
                <br>
            </div>
            <div>
                <input type="hidden" class="form-control" name="tipo_usuario" 
                placeholder="tipo_usuario"
                value="{{ request.form['tipo_usuario'] }}">
                <br>
            </div> 




{% endblock %}


